{
  "name": "mtx-sidecar", "version": "0.0.0",
  "dependencies": {
    "@sap/cds-mtxs": "*",
    "express": "*"
  },
  "cds": {
    "requires": {
      "cds.xt.ModelProviderService": "in-sidecar",
      "cds.xt.SaasProvisioningService": true,
      "-cds.xt.DeploymentService": true,
      "-cds.xt.ExtensibilityService": true,
      "multitenancy": true,
      "db": {
        "kind": "sqlite",
        "credentials": {
          "url": "../../sqlite.db"
        }
      },
      "[mps-from-main]": {
        "cds.xt.ModelProviderService": {
          "kind": "rest",
          "-credentials": {
            "url": "http://localhost:4004/-/cds/model-provider",
            "username": "yves",
            "password": "whatever"
          }
        }
      }
    },
    "[development]": {
      "requires": { "auth": "dummy" },
      "server": { "port": 4005 }
    }
  }
}
