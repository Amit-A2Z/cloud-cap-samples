using Z_BOOKSHOP_EXTENSION_Z_CUSTOMERS as Z_BOOKSHOP_EXTENSION_Z_CUSTOMERS;
using LOCALIZED_Z_BOOKSHOP_EXTENSION_Z_CUSTOMERPOSTALADDRESSES as LOCALIZED_Z_BOOKSHOP_EXTENSION_Z_CUSTOMERPOSTALADDRESSES;
view LOCALIZED_Z_BOOKSHOP_EXTENSION_Z_CUSTOMERS as select from Z_BOOKSHOP_EXTENSION_Z_CUSTOMERS as L mixin {
  ___POSTALADDRESSES : association[*] to LOCALIZED_Z_BOOKSHOP_EXTENSION_Z_CUSTOMERPOSTALADDRESSES on ___POSTALADDRESSES.CUSTOMER_ID = $projection.ID;
} into {
  key L.ID as ID,
  L.CREATEDAT,
  L.CREATEDBY,
  L.MODIFIEDAT,
  L.MODIFIEDBY,
  L.EMAIL,
  L.FIRSTNAME,
  L.LASTNAME,
  L.CREDITCARDNO,
  L.DATEOFBIRTH,
  L.STATUS,
  L.CREDITSCORE,
  ___POSTALADDRESSES as POSTALADDRESSES
};
