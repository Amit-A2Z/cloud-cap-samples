using Z_BOOKSHOP_EXTENSION_Z_SALESREGION_TEXTS as Z_BOOKSHOP_EXTENSION_Z_SALESREGION_TEXTS;
entity Z_BOOKSHOP_EXTENSION_Z_SALESREGION {
  NAME : String(255);
  DESCR : String(1000);
  key REGIONCODE : String(11);
  TEXTS : association[*] to Z_BOOKSHOP_EXTENSION_Z_SALESREGION_TEXTS on TEXTS.REGIONCODE = REGIONCODE;
  LOCALIZED : association to Z_BOOKSHOP_EXTENSION_Z_SALESREGION_TEXTS on LOCALIZED.REGIONCODE = REGIONCODE and LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
};
