using Z_BOOKSHOP_EXTENSION_Z_SALESREGION as Z_BOOKSHOP_EXTENSION_Z_SALESREGION;
using ORDERSSERVICE_Z_SALESREGION_TEXTS as ORDERSSERVICE_Z_SALESREGION_TEXTS;
view ORDERSSERVICE_Z_SALESREGION as select from Z_BOOKSHOP_EXTENSION_Z_SALESREGION as Z_SALESREGION_0 mixin {
  ___TEXTS : association[*] to ORDERSSERVICE_Z_SALESREGION_TEXTS on ___TEXTS.REGIONCODE = $projection.REGIONCODE;
  ___LOCALIZED : association to ORDERSSERVICE_Z_SALESREGION_TEXTS on ___LOCALIZED.REGIONCODE = $projection.REGIONCODE and ___LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
} into {
  Z_SALESREGION_0.NAME,
  Z_SALESREGION_0.DESCR,
  key Z_SALESREGION_0.REGIONCODE as REGIONCODE,
  ___TEXTS as TEXTS,
  ___LOCALIZED as LOCALIZED
};
